{
    "extends": [
        "airbnb",
        "airbnb-typescript",
        "plugin:@typescript-eslint/recommended",
        "plugin:@typescript-eslint/recommended-requiring-type-checking"
    ],
    "rules": {
        "prefer-template": "error",
        "no-void": "off",
        "@typescript-eslint/explicit-module-boundary-types": 0  
    },
    "parserOptions": {
        "project": "./tsconfig.json"
    }
}